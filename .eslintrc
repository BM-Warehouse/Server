{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "import"
  ],
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "no-shadow": "off",
    "no-console":"warn",
    "max-len": [
      "error",
      100
    ],
    "import/no-named-default": "off",
    "import/prefer-default-export": "off",
    "quotes": [
      "error",
      "single"
    ],
    "semi": "error"
  }
}